{
  "manifest_version": "1.0",
  "extensions": [
    {
      "epID": "console.command",
      "command": "list plugins",
      "function": "./lib/console!listPlugins",
      "description": "List the plugins that are currently loaded"
    },
    {
      "epID": "console.command",
      "command": "list singletons",
      "function": "./lib/console!listSingletons",
      "description": "List the singletons that are currently available"
    },
    {
      "epID": "console.command",
      "command": "load plugin",
      "function": "./lib/console!loadPlugin",
      "description": "Load a new plugin"
    },
    {
      "epID": "console.command",
      "command": "unload plugin",
      "function": "./lib/console!unloadPlugin",
      "description": "Unload an installed plugin"
    },
    {
      "epID": "console.command",
      "command": "restart plugin",
      "function": "./lib/console!restartPlugin",
      "description": "Unload then load an installed plugin"
    },
    {
      "epID": "console.command",
      "command": "list ep",
      "function": "./lib/console!listExtensionPoints",
      "description": "List the extension points available to extend"
    }
  ],

  "permissions": [
    "pluginManager"
  ]
}